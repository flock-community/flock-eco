{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "rootDirs": [
      "src/main/frontend",
      "generated/src/main/frontend"
    ],
    "outDir": "./lib/"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}